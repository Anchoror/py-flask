{
    "swagger": "2.0",
    "info": {
        "title": "Flask API",
        "version": "1.0.0"
    },
    "basePath": "/api",
    "paths": {
        "/users": {
            "get": {
                "summary": "获取用户列表",
                "responses": {
                    "200": {
                        "description": "用户列表"
                    }
                }
            },
            "post": {
                "summary": "创建新用户",
                "parameters": [
                    {
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "username": {"type": "string"},
                                "email": {"type": "string"},
                                "password": {"type": "string"}
                            }
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "用户创建成功"
                    }
                }
            }
        }
    }
}